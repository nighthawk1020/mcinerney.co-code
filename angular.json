{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "",
  "projects": {
    "angular": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular",
      "sourceRoot": "apps/angular/src",
      "prefix": "angular",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular",
            "index": "apps/angular/src/index.html",
            "main": "apps/angular/src/main.ts",
            "polyfills": "apps/angular/src/polyfills.ts",
            "tsConfig": "apps/angular/tsconfig.app.json",
            "assets": [
              "apps/angular/src/favicon.ico",
              "apps/angular/src/assets"
            ],
            "styles": ["apps/angular/src/styles.scss"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular/src/environments/environment.ts",
                  "with": "apps/angular/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular:build",
            "proxyConfig": "apps/angular/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular/tsconfig.app.json",
              "apps/angular/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular/jest.config.js",
            "tsConfig": "apps/angular/tsconfig.spec.json",
            "setupFile": "apps/angular/src/test-setup.ts"
          }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "tsConfig": "apps/api/tsconfig.spec.json"
          }
        }
      }
    },
    "react": {
      "root": "apps/react",
      "sourceRoot": "apps/react/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react",
            "index": "apps/react/src/index.html",
            "main": "apps/react/src/main.tsx",
            "polyfills": "apps/react/src/polyfills.ts",
            "tsConfig": "apps/react/tsconfig.app.json",
            "assets": ["apps/react/src/favicon.ico", "apps/react/src/assets"],
            "styles": ["apps/react/src/styles.scss"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react/src/environments/environment.ts",
                  "with": "apps/react/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/react/tsconfig.app.json",
              "apps/react/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react/jest.config.js",
            "tsConfig": "apps/react/tsconfig.spec.json"
          }
        }
      }
    },
    "crowd-dj": {
      "projectType": "application",
      "schematics": {
        "@nrwl/workspace:component": {
          "style": "scss"
        }
      },
      "root": "apps/crowd-dj",
      "sourceRoot": "apps/crowd-dj/src",
      "prefix": "angular",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/crowd-dj",
            "index": "apps/crowd-dj/src/index.html",
            "main": "apps/crowd-dj/src/main.ts",
            "polyfills": "apps/crowd-dj/src/polyfills.ts",
            "tsConfig": "apps/crowd-dj/tsconfig.app.json",
            "assets": [
              "apps/crowd-dj/src/favicon.ico",
              "apps/crowd-dj/src/assets"
            ],
            "styles": ["apps/crowd-dj/src/styles.scss"]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/crowd-dj/src/environments/environment.ts",
                  "with": "apps/crowd-dj/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "crowd-dj:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "crowd-dj:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "crowd-dj:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/crowd-dj/tsconfig.app.json",
              "apps/crowd-dj/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/crowd-dj/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/crowd-dj/jest.config.js",
            "tsConfig": "apps/crowd-dj/tsconfig.spec.json",
            "setupFile": "apps/crowd-dj/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "warnings": {
      "typescriptMismatch": false,
      "versionMismatch": false
    },
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "angular"
}
